<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/common_control_solid" />
            <corners android:radius="@dimen/common_control_corner_radius" />
            <padding android:left="10dp"></padding>
            <stroke android:width="@dimen/common_control_stroke_width" android:color="@color/common_control_stroke_focus" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/common_control_solid" />
            <corners android:radius="@dimen/common_control_corner_radius" />
            <padding android:left="10dp"></padding>
            <stroke android:width="@dimen/common_control_stroke_width" android:color="@color/common_control_stroke" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/common_control_solid" />
            <corners android:radius="@dimen/common_control_corner_radius" />
            <padding android:left="10dp"></padding>
            <stroke android:width="@dimen/common_control_stroke_width" android:color="@color/common_control_stroke" />
        </shape>
    </item>
</selector>